{
  "name": "@ictroot/ui-kit",
  "version": "0.0.2-beta.4",
  "description": "Modular and expanded library of components created using Vite, Storybook and Typescript. Suitable for re-used UI elements in various projects",
  "author": "IctRoot",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/ui-kit.cjs.js",
  "module": "dist/ui-kit.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-kit.es.js",
      "require": "./dist/ui-kit.cjs.js"
    },
    "./style.css": "./dist/ui-kit.css"
  },
  "files": [
    "dist",
    "dist/*.css"
  ],
  "sideEffects": [
    "./dist/ui-kit.css"
  ],
  "scripts": {
    "nps": "nps",
    "dev": "nps dev",
    "generate-index": "nps generateIndex",
    "version:patch": "nps version.patch",
    "commit": "node scripts/git-commit-message.cjs",
    "generateIndex": "node scripts/generate-index.js",
    "build": "nps generateIndex && vite build && tsc",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ictroot70/ui-kit.git"
  },
  "contributors": [
    "Smbat Antonyan <sem.antony.dev@gmail.com>",
    "Muzalev Dmitrii <muzalev771@gmail.com>",
    "Krasko Aleksei <alekskrasko1985@gmail.com>",
    "Kenessary Amanbay <koni9812@gmail.com>",
    "Denis Denisov <denis@example.com>",
    "Denis Denisov <denis@example.com>",
    "Denis Denisov <denis@example.com>",
    "etc."
  ],
  "bugs": {
    "url": "https://github.com/ictroot70/ui-kit/issues"
  },
  "homepage": "https://github.com/ictroot70/ui-kit#readme",
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.2.5",
    "@radix-ui/react-checkbox": "1.3.1",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.13",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-scroll-area": "1.2.8",
    "@radix-ui/react-select": "2.2.4",
    "@radix-ui/react-slot": "1.2.2",
    "@radix-ui/react-tabs": "1.1.11",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "clsx": "2.1.1",
    "framer-motion": "12.12.1",
    "lucide-react": "0.510.0",
    "react-day-picker": "9.6.7",
    "react-google-recaptcha": "3.1.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.6",
    "@it-incubator/eslint-config": "^1.0.4",
    "@it-incubator/prettier-config": "^0.1.2",
    "@it-incubator/stylelint-config": "^2.0.0",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-onboarding": "8.6.12",
    "@storybook/addon-storysource": "8.6.14",
    "@storybook/blocks": "8.6.12",
    "@storybook/experimental-addon-test": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@types/node": "22.15.17",
    "@types/react": "18.3.21",
    "@types/react-dom": "18.3.7",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^0.12.0",
